{
  "name": "pundit",
  "version": "1.8.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:mocha": "mocha -r ts-node/register --watch --watch-files src, src/**/*.spec.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "stylelint": "stylelint \"src/styles/**/*.scss\"; exit 0",
    "stylelint:watch": "onchange \"src/styles/**/*.scss\" -- npm run stylelint",
    "build:chrome-ext-stage": "npm run build:embed-stage && ng build -c chrome-ext-stage --single-bundle",
    "postbuild:chrome-ext-stage": "webpack --config webpack.chrome-ext.stage.js && node ./scripts/postbuild.js -c chrome-ext-stage",
    "build:chrome-ext-prod": "npm run build:embed-prod && ng build -c chrome-ext-prod --single-bundle",
    "postbuild:chrome-ext-prod": "webpack --config webpack.chrome-ext.prod.js && node ./scripts/postbuild.js -c chrome-ext-prod",
    "build:embed-stage": "ng build -c embed-stage --single-bundle",
    "postbuild:embed-stage": "node ./scripts/postbuild.js -c embed-stage",
    "build:embed-prod": "ng build -c embed-prod --single-bundle",
    "postbuild:embed-prod": "node ./scripts/postbuild.js -c embed-prod",
    "build:pdf-standalone-stage": "ng build -c pdf-standalone-stage --single-bundle",
    "postbuild:pdf-standalone-stage": "node ./scripts/postbuild.js -c pdf-standalone-stage",
    "build:pdf-standalone-prod": "ng build -c pdf-standalone-prod --single-bundle",
    "postbuild:pdf-standalone-prod": "node ./scripts/postbuild.js -c pdf-standalone-prod",
    "update:framework": "npm update @n7-frontend/components @n7-frontend/boilerplate @net7/core @n7-frontend/icon-font"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/elements": "^12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@net7/core": "^3.0.0",
    "@popperjs/core": "^2.6.0",
    "@pundit/anchoring": "git+ssh://git@github.com/net7/pundit-anchoring.git#develop",
    "@pundit/common": "git+ssh://git@github.com/net7/pundit-common.git#develop",
    "@pundit/communication": "git+ssh://git@github.com/net7/pundit-communication.git#develop",
    "@pundit/icon-font": "git+ssh://git@github.com/net7/pundit-icon-font.git#v1.1.0",
    "@yaireo/tagify": "4.3.0",
    "approx-string-match": "^1.1.0",
    "axios": "^0.21.0",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.7.2",
    "draggable": "^4.2.0",
    "faker": "^5.1.0",
    "lodash": "^4.17.20",
    "mixpanel-browser": "^2.41.0",
    "ngx-build-plus": "~10.1.1",
    "pdfjs-dist": "~2.5.207",
    "prosemirror-commands": "~1.1.10",
    "prosemirror-history": "~1.2.0",
    "prosemirror-inputrules": "~1.1.3",
    "prosemirror-keymap": "~1.1.4",
    "prosemirror-schema-basic": "~1.1.2",
    "prosemirror-schema-list": "~1.1.5",
    "prosemirror-model": "1.16.0",
    "prosemirror-state": "1.3.4",
    "prosemirror-transform": "1.3.2",
    "prosemirror-view": "1.23.13",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.6.0",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.2.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@types/chrome": "0.0.127",
    "@types/faker": "^5.1.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "4.14.179",
    "@types/mixpanel-browser": "^2.35.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "fs-extra": "^9.1.0",
    "html-loader": "^1.3.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.0.8",
    "minimist": "^1.2.5",
    "onchange": "^6.1.0",
    "protractor": "~7.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "8.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack": "4.42.0",
    "webpack-cli": "^4.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  }
}
