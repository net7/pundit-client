{
  "name": "pundit",
  "version": "2.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:mocha": "mocha -r ts-node/register --watch --watch-files src, src/**/*.spec.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "stylelint": "stylelint \"src/styles/**/*.scss\"; exit 0",
    "stylelint:watch": "onchange \"src/styles/**/*.scss\" -- npm run stylelint",
    "update:framework": "npm update @n7-frontend/components @n7-frontend/boilerplate @n7-frontend/core @n7-frontend/icon-font"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@n7-frontend/components": "git+ssh://git@github.com/net7/n7-frontend-components-dist.git#master",
    "@n7-frontend/core": "git+ssh://git@github.com/net7/n7-frontend-core.git#master",
    "@n7-frontend/icon-font": "git+ssh://git@github.com/net7/n7-icon-font.git#master",
    "@pundit/common": "git+ssh://git@github.com/net7/pundit-common.git#develop",
    "@pundit/communication": "git+ssh://git@github.com/net7/pundit-communication.git#develop",
    "@pundit/anchoring": "git+ssh://git@github.com/net7/pundit-anchoring.git#develop",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.7.2",
    "lodash": "^4.17.20",
    "rxjs": "6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "chai": "^4.2.0",
    "codelyzer": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.0.8",
    "mocha": "^8.2.1",
    "onchange": "^6.1.0",
    "protractor": "~5.4.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  }
}
