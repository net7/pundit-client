{
  "name": "pundit",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:mocha": "mocha -r ts-node/register --watch --watch-files src, src/**/*.spec.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "stylelint": "stylelint \"src/styles/**/*.scss\"; exit 0",
    "stylelint:watch": "onchange \"src/styles/**/*.scss\" -- npm run stylelint",
    "build:chrome-ext-stage": "npm run build:embed-stage && ng build -c chrome-ext-stage --single-bundle",
    "postbuild:chrome-ext-stage": "webpack --config webpack.chrome-ext.js && node ./scripts/postbuild.js -c chrome-ext-stage",
    "build:chrome-ext-prod": "ng build -c chrome-ext-prod --single-bundle",
    "postbuild:chrome-ext-prod": "webpack --config webpack.chrome-ext.js && node ./scripts/postbuild.js -c chrome-ext-prod",
    "build:embed-stage": "ng build -c embed-stage --single-bundle",
    "watch:embed-stage": "npm run build:embed-stage -- --watch",
    "postbuild:embed-stage": "node ./scripts/postbuild.js -c embed-stage",
    "build:embed-prod": "ng build -c embed-prod --single-bundle",
    "watch:embed-prod": "npm run build:embed-prod -- --watch",
    "postbuild:embed-prod": "node ./scripts/postbuild.js -c embed-prod",
    "update:framework": "npm update @n7-frontend/components @n7-frontend/boilerplate @n7-frontend/core @n7-frontend/icon-font"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@n7-frontend/core": "git+ssh://git@github.com/net7/n7-frontend-core.git#master",
    "@popperjs/core": "^2.6.0",
    "@pundit/anchoring": "git+ssh://git@github.com/net7/pundit-anchoring.git#develop",
    "@pundit/common": "git+ssh://git@github.com/net7/pundit-common.git#develop",
    "@pundit/communication": "git+ssh://git@github.com/net7/pundit-communication.git#develop",
    "@pundit/icon-font": "git+ssh://git@github.com/net7/pundit-icon-font.git#v1.1.0",
    "@yaireo/tagify": "^4.2.0",
    "approx-string-match": "^1.1.0",
    "axios": "^0.21.0",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.7.2",
    "draggable": "^4.2.0",
    "faker": "^5.1.0",
    "lodash": "^4.17.20",
    "mixpanel-browser": "^2.41.0",
    "ngx-build-plus": "~10.1.1",
    "pdfjs-dist": "~2.5.207",
    "prosemirror-commands": "^1.1.10",
    "prosemirror-history": "^1.2.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-model": "^1.14.3",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-schema-list": "^1.1.5",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.19.3",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "6.5.5",
    "seedrandom": "^3.0.5",
    "tippy.js": "^6.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@types/chrome": "0.0.127",
    "@types/faker": "^5.1.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.169",
    "@types/mixpanel-browser": "^2.35.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "codelyzer": "^5.1.2",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "fs-extra": "^9.1.0",
    "html-loader": "^1.3.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.0.8",
    "minimist": "^1.2.5",
    "onchange": "^6.1.0",
    "protractor": "~5.4.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "8.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "^4.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  }
}
